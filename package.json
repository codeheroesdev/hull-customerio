{
  "name": "hull-customerio",
  "description": "Sync users within customer.io service",
  "version": "0.1.0",
  "homepage": "https://github.com/hull-ships/hull-customerio",
  "license": "MIT",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-customerio"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-customerio/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "NODE_ENV=development babel-watch -L server/index",
    "start:dev": "NODE_ENV=development LOG_LEVEL=debug babel-node -- server",
    "test": "npm run test:lint && npm run test:units && npm run test:specs",
    "test:lint": "eslint server",
    "test:unit": "NODE_ENV=test mocha -R spec ./tests/unit",
    "test:integration": "NODE_ENV=test mocha -R spec ./tests/integration",
    "clean": "rimraf dist; rimraf lib"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship",
    "connector"
  ],
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bluebird": "^3.4.1",
    "bottleneck": "^1.16.0",
    "express": "^4.15.3",
    "hull": "0.11.8",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-watch": "^2.0.5",
    "babel-loader": "^6.2.4",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "minihull": "^2.0.0",
    "mocha": "^3.4.2",
    "sinon": "^1.17.4"
  }
}
